
proc SETISWELLDEFINED {
  home
  position -100, 100
  color red
  fw PI
  set PI 100
  color blue
  fw PI
}



proc FORWARDRIGHT{
  fw DISTANCE
  right ANGLE

}

set DISTANCE -10 ^ 2
set PI 60
set ANGLE PI * 2

color red
call FORWARDRIGHT
color 0, 1, 0
call FORWARDRIGHT
color blue
call FORWARDRIGHT
call SETISWELLDEFINED
